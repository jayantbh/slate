struct node* getEmptyList()
{
    struct node* head = (struct node*) malloc(sizeof(struct node));
    struct node* body = (struct node*) malloc(sizeof(struct node));
    head->data = '\r';
    head->prev = NULL;
    head->next = body;
    body->prev = head;
    body->next = NULL;
    return getHead(body);
}
